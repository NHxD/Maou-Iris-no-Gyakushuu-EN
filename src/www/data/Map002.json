                        {"code":111,"indent":0,"parameters":[1,11,0,0,0]},
                        {"code":105,"indent":1,"parameters":[2,true]},
                        {"code":405,"indent":1,"parameters":["\\m魔王イブリースの脅威によって世界は暗黒の地と化し、"]},
                        {"code":405,"indent":1,"parameters":["\\m邪悪な魔の手に支配されかけていた。"]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\mだが人々が絶望し、魔王の足元に完全にひれ伏しかけた"]},
                        {"code":405,"indent":1,"parameters":["\\mその時、一人の勇者が忽然と現れる。"]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\m勇者――その名を、シオン。"]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\mシオンは立ちはだかる魔物たちを次々と倒して人々を"]},
                        {"code":405,"indent":1,"parameters":["\\m守りながら、驚くばかりの戦果をあげて人類の"]},
                        {"code":405,"indent":1,"parameters":["\\m劣勢を跳ね返していく。"]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\m…………そして、ついに魔王イブリースとの"]},
                        {"code":405,"indent":1,"parameters":["\\m最終決戦が始まる。"]},
                        {"code":0,"indent":1,"parameters":[]},
                        {"code":412,"indent":0,"parameters":[]},
                        {"code":111,"indent":0,"parameters":[1,11,0,1,0]},
                        {"code":105,"indent":1,"parameters":[2,true]},
                        {"code":405,"indent":1,"parameters":["\\mThreatened by the Devil Iblis,"]},
                        {"code":405,"indent":1,"parameters":["\\mthe world fell into darkness."]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\mAs the Devil was about to"]},
                        {"code":405,"indent":1,"parameters":["\\mrule over the entire world..."]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\mA hero suddenly appeared."]},
                        {"code":405,"indent":1,"parameters":["\\mChiot, was that man's name."]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\mChiot returned hope to the world"]},
                        {"code":405,"indent":1,"parameters":["\\mby defeating any monsters that stood"]},
                        {"code":405,"indent":1,"parameters":["\\min his way, one after another."]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\mIt wasn't long before"]},
                        {"code":405,"indent":1,"parameters":["\\mthe tables were turned in humanity's favor."]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\m... Finally, the battle against the Devil begins."]},
                        {"code":0,"indent":1,"parameters":[]},
                        {"code":412,"indent":0,"parameters":[]},
                        {"code":111,"indent":0,"parameters":[1,11,0,2,0]},
                        {"code":105,"indent":1,"parameters":[2,true]},
                        {"code":405,"indent":1,"parameters":["\\m在魔王伊利絲的威脅下世界陷入黑暗、"]},
                        {"code":405,"indent":1,"parameters":["\\m壹切即將落入邪惡的魔王手中。"]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\m就在人們逐漸絕望、徹底拜服在魔王的統治之下時、"]},
                        {"code":405,"indent":1,"parameters":["\\m壹名勇者突然出現了。"]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\m這位勇者――他的名字、叫做西歐。"]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\m西歐將擋在自己身前的魔物們接連打倒並守護了人民、"]},
                        {"code":405,"indent":1,"parameters":["\\m那無與倫比的戰果"]},
                        {"code":405,"indent":1,"parameters":["\\m使人類瞬間逆轉了劣勢。"]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":[""]},
                        {"code":405,"indent":1,"parameters":["\\m…………之後、終於到了與魔王伊利絲"]},
                        {"code":405,"indent":1,"parameters":["\\m進行最終決戰的時刻"]},
